{
    "type": "object",
    "properties": {
        "login-email": {
            "type": "string",
            "pattern": "^[a-zA-Z0-9.!#$%&'*+\/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$",
            "minLength": 6,
            "maxLength": 127
        },
        "login-password": {
            "type": "string"
        }
    },
    "required": ["login-email", "login-password"],
    "additionalProperties": false
}
